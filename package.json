		"@types/react-dom": "^19",
		"eslint": "^9",
		"eslint-config-next": "^15.4.6",
		"npm-check-updates": "^18",
		"tailwindcss": "^4",
		"typescript": "^5",
		"wrangler": "^4.28.1"
	},
	"overrides": {
		"cookie": "^1.0.2",
