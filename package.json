{
	"name": "www-afrotc-org",
	"version": "2.0.2",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"test": "vitest run",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
		"preview:dev": "opennextjs-cloudflare build && opennextjs-cloudflare preview -- --env=dev",
		"deploy:dev": "opennextjs-cloudflare build && opennextjs-cloudflare deploy -- --env=dev",
		"upload:dev": "opennextjs-cloudflare build && opennextjs-cloudflare upload -- --env=dev",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"upgrade": "npx npm-check-updates -u && npm install"
	},
	"dependencies": {
		"@opennextjs/cloudflare": "^1.6.5",
		"next": "15.4.6",
		"react": "^19.1.1",
		"react-dom": "^19.1.1"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^24.2.1",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"eslint": "^9",
		"eslint-config-next": "^15.4.6",
		"npm-check-updates": "^18.0.2",
		"tailwindcss": "^4",
		"typescript": "^5",
		"vitest": "^2.1.9",
		"wrangler": "^4.28.1"
	}
}
